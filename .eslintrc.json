{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "import/prefer-default-export": 0,
        "no-irregular-whitespace": [
            "error",
            {
                "skipRegExps": true
            }
        ],
        "class-methods-use-this": 0,
        "no-undef": 0
    }
}
